    {
      "@id": "https://data.vlaanderen.be/ns/openbaardomein/vegetatie#Muurvegetatie",
      "rdfs:label": {
        "@language": "nl",
        "@value": "Muurvegetatie"
      },
      "rdfs:subClassOf": {
        "@id": "https://data.vlaanderen.be/ns/openbaardomein/vegetatie#VegetatieElement"
      },
      "@type": "owl:Class",
      "rdfs:isDefinedBy": {
        "@id": "https://data.vlaanderen.be/ns/openbaardomein/vegetatie"
      },
      "rdfs:comment": {
        "@language": "nl",
        "@value": "Muurvegetaties zijn gebonden aan door de mens gecreëerde stenige, doorgaans steile tot verticale standplaatsen. Voorbeelden zijn kerkhofmuren, stadswallen, ruïnes, kademuren, oude forten en bunkers."
      },
      "http://purl.org/vocab/vann/usageNote": {
        "@language": "nl",
        "@value": "Definitie naar Ecopedia."
      }
    },
