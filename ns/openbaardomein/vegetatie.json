        "@language": "nl"
      },
      "rdfs:comment": {
        "@value": "Individueel te onderscheiden vegetatie-element. Een boom is een overblijvende plant met een verhoute stam en een kroon (kruin).",
        "@language": "nl"
      },
      "rdfs:isDefinedBy": {
        "@id": "http://data.vlaanderen.be/ns/openbaardomein/vegetatie"
      }
