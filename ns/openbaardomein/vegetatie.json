    {
      "@id": "https://data.vlaanderen.be/ns/openbaardomein/vegetatie#Plantbakvorm",
      "rdfs:label": {
        "@value": "Plantbakvorm",
        "@language": "nl"
      },
      "rdfs:comment": {
        "@value": "Beplanting die niet in volle grond werd aangebracht, maar in bakvorm.",
        "@language": "nl"
      },
      "@type": "owl:Class",
      "rdfs:isDefinedBy": {
