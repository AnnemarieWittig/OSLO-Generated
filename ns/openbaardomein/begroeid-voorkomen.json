      },
      "@type": "owl:Class"
    },
    {
      "@id": "http://data.vlaanderen.be/ns/openbaardomein/begroeidvoorkomen#Bomen",
      "rdfs:label": {
        "@value": "Bomen",
        "@language": "nl"
      },
      "rdfs:comment": {
        "@value": "Opgaande beplanting van houtachtige gewassen die boomvormend zijn.",
        "@language": "nl"
      },
      "rdfs:isDefinedBy": {
        "@id": "http://data.vlaanderen.be/ns/openbaardomein/begroeidvoorkomen"
      },
