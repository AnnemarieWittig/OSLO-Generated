    {
      "@id": "http://data.vlaanderen.be/ns/openbaardomein/begroeidvoorkomen#BolEnKnolgewassen",
      "rdfs:label": {
        "@value": "Bol- en knolgewassen",
        "@language": "nl"
      },
      "rdfs:subClassOf": {
        "@id": "http://data.vlaanderen.be/ns/openbaardomein/begroeidvoorkomen#Kruidachtigen"
      },
      "rdfs:comment": {
        "@value": "Bol- en knolgewassen beschikken over een ondergronds voedselreservoir in de vorm van een bol of een knol. Veelal verschijnen de bovengrondse delen in een specifieke periode van het jaar en kennen zij een geweldige bloei.",
        "@language": "nl"
      },
      "rdfs:isDefinedBy": {
        "@id": "http://data.vlaanderen.be/ns/openbaardomein/begroeidvoorkomen"
      },
      "@type": "owl:Class"
    },
    {
      "@id": "http://data.vlaanderen.be/ns/openbaardomein/begroeidvoorkomen#Bloemenakker",
      "rdfs:label": {
        "@value": "Bloemenakker",
        "@language": "nl"
      },
      "rdfs:subClassOf": {
        "@id": "http://data.vlaanderen.be/ns/openbaardomein/begroeidvoorkomen#Grasachtigen"
      },
      "rdfs:comment": {
        "@value": "Grasland dat is ingezaaid met wilde- of half wilde soorten waarvan de bodem elk jaar moet geschoffeld of geploegd worden.",
        "@language": "nl"
      },
      "rdfs:isDefinedBy": {
        "@id": "http://data.vlaanderen.be/ns/openbaardomein/begroeidvoorkomen"
      },
