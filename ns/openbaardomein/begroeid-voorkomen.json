    {
      "@id": "http://data.vlaanderen.be/ns/openbaardomein/begroeidvoorkomen#Houtkant",
      "rdfs:label": {
        "@value": "Houtkant",
        "@language": "nl"
      },
      "rdfs:subClassOf": {
        "@id": "http://data.vlaanderen.be/ns/openbaardomein/begroeidvoorkomen#Houtachtigen"
      },
      "rdfs:comment": {
        "@value": "Ongeschoren haag of mantelvegetatie, die soms als hakhout wordt behandeld, maar niet op een door de mens aangelegde wal (verhoging).",
        "@language": "nl"
      },
      "http://purl.org/vocab/vann/usageNote": {
        "@value": "Bron definitie: Vlaams agentschap voor Natuur en Bos",
