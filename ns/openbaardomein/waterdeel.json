      },
      "rdfs:comment": {
        "@value": "Gebied dat rechtstreeks gedomineerd wordt door de tijdelijke of permanente aanwezigheid van oppervlaktewater, met een bepaalde hieraan verbonden functie",
        "@language": "nl"
      },
      "rdfs:label": {
        "@value": "Waterdeel",
        "@language": "nl"
      },
      "http://purl.org/vocab/vann/usageNote": {
        "@value": "GRB Lexicon: Definitie Watergang",
