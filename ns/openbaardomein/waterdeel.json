        "@value": "GRB Lexicon: Definitie Watergang",
        "@language": "nl"
      },
      "@type": "owl:Class",
      "rdfs:label": {
        "@value": "Waterdeel",
        "@language": "nl"
      },
      "rdfs:comment": {
        "@value": "Gebied dat rechtstreeks gedomineerd wordt door de tijdelijke of permanente aanwezigheid van oppervlaktewater, met een bepaalde hieraan verbonden functie",
        "@language": "nl"
      },
      "rdfs:subClassOf": {
        "@id": "https://data.vlaanderen.be/ns/openbaardomein#Deel"
      }
    },
    {
      "@id": "https://data.vlaanderen.be/ns/openbaardomein/waterdeel#Gracht",
