        "@id": "https://data.vlaanderen.be/ns/openbaardomein/vegetatie"
      },
      "rdfs:comment": {
        "@value": "Een zelfhechtende klimplant zoals klimop of wingerd groeit rechtstreeks op de muur zonder nood aan een draagstructuur.",
        "@language": "nl"
      },
      "http://purl.org/vocab/vann/usageNote": {
        "@value": "Definitie vrij naar Ecopedia: https://www.ecopedia.be/encyclopedie/groene-gevel",
        "@language": "nl"
      }
    },
    {
      "@id": "https://data.vlaanderen.be/ns/openbaardomein/vegetatie#Klimvorm",
      "rdfs:label": {
        "@value": "Klimvorm",
        "@language": "nl"
      },
      "rdfs:subClassOf": {
        "@id": "https://data.vlaanderen.be/ns/openbaardomein/vegetatie#VegetatieElement"
      },
      "@type": "owl:Class",
      "rdfs:isDefinedBy": {
        "@id": "https://data.vlaanderen.be/ns/openbaardomein/vegetatie"
      },
      "rdfs:comment": {
        "@value": "Plant met buigzame stengels die zich aan muren, bomen, enz. hecht en zodoende omhoog klimt.",
        "@language": "nl"
      }
    },
