        "@value": "Solitaire Heester",
        "@language": "nl"
      },
      "rdfs:comment": {
        "@value": "Afzonderlijk te onderscheiden heester.",
        "@language": "nl"
      },
      "@type": "owl:Class",
      "rdfs:subClassOf": {
        "@id": "http://data.vlaanderen.be/ns/openbaardomein/vegetatie#VegetatieElement"
