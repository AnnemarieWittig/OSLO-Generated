      "rdfs:label": {
        "@value": "bereikbaar",
        "@language": "nl"
      },
      "rdfs:comment": {
        "@value": "Duidt aan of het element door de mens fysiek bereikbaar is zonder hulpmiddelen.",
        "@language": "nl"
      },
      "rdfs:range": {
        "@id": "xsd:boolean"
      },
      "@type": "owl:DatatypeProperty",
      "rdfs:domain": {
