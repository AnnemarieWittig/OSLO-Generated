      }
    },
    {
      "@id": "https://data.vlaanderen.be/ns/openbaardomein/terreindeel#Wegbaan",
      "http://purl.org/vocab/vann/usageNote": {
        "@value": "Naar GRB-basis - WBN: Rand van de wegbaan",
        "@language": "nl"
      },
      "rdfs:comment": {
        "@value": "De wegbaan (Wbn) is de voorstelling van de wegcorridor in het GRB. De wegbaan wordt overlangs opgedeeld in verschillende arbitrair afgebakende wegbaanelementen. Er zijn twee types wegbaanelementen: een kruispuntzone komt overeen met een kruispunt; een wegsegment komt overeen met een wegvak.",
        "@language": "nl"
      },
      "rdfs:isDefinedBy": {
        "@id": "https://data.vlaanderen.be/ns/openbaardomein/terreindeel"
      },
      "rdfs:label": {
        "@value": "Wegbaan",
        "@language": "nl"
      },
      "@type": "owl:Class",
