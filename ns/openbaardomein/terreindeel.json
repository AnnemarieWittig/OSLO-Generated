        "@id": "https://data.vlaanderen.be/ns/openbaardomein#Deel"
      },
      "rdfs:label": {
        "@value": "Terreindeel",
        "@language": "nl"
      },
      "rdfs:comment": {
        "@value": "Gedeelte van het aardoppervlak, met een gelijkaardige functie, dat geen deel uitmaakt van 'waterdeel'",
        "@language": "nl"
      },
      "rdfs:isDefinedBy": {
        "@id": "https://data.vlaanderen.be/ns/openbaardomein/terreindeel"
      },
      "@type": "owl:Class"
    },
