      rdfs:range <http://data.vlaanderen.be/ns/persoon#Nationaliteit> ;
      vann:usageNote " "@nl .

<http://data.vlaanderen.be/ns/persoon#heeftOverlijden>
      a       owl:ObjectProperty ;
