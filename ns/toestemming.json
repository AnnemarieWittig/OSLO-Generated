      "rdfs:isDefinedBy": {
        "@id": "https://data.vlaanderen.be/ns/toestemming"
      },
      "rdfs:comment": {
        "@language": "nl",
        "@value": "Standaardbedingen waarnaar verwezen kan worden bij het opstellen van een schriftelijke overeenkomst. Algemene voorwaarden mogen niet 'onredelijk bezwarend' zijn; dan kunnen ze door de rechter nietig worden verklaard."
      },
      "rdfs:subClassOf": {
        "@id": "foaf:Document"
      }
    },
