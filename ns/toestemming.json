    {
      "@id": "https://data.vlaanderen.be/ns/toestemming#Toestemming",
      "@type": "owl:Class",
      "rdfs:comment": {
        "@language": "nl",
        "@value": "Een expliciete toestemming,gegeven door een bepaalde Agent die het onderwerp vormt van de persoonlijke informatie."
