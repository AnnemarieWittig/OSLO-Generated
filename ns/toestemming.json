      },
      "rdfs:subClassOf": {
        "@id": "foaf:Document"
      },
      "rdfs:label": {
        "@value": "Verwerkersovereenkomst",
        "@language": "nl"
      },
      "http://purl.org/vocab/vann/usageNote": {
