    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Nationaliteit",
      "rdfs:comment": {
        "@value": "De relatie tussen een persoon en een staat, cultuur of loyaliteit.",
        "@language": "nl"
      },
      "rdfs:isDefinedBy": {
