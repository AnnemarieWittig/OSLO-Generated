    {
      "@id": "http://data.vlaanderen.be/ns/persoon#Afstamming",
      "@type": "owl:Class",
      "http://purl.org/vocab/vann/usageNote": {
        "@language": "nl",
        "@value": "De afstamming kan ook niet-biologisch zijn, bv adoptie. Afstamming kan ook in opgaande lijn worden bekeken. Het begrip is enger dan verwantschap dat oa ook huwelijk inhoudt."
      },
      "rdfs:comment": {
        "@language": "nl",
        "@value": "Afstamming is de dalende lijn van bloedverwantschap tussen verschillende generaties."
      },
      "rdfs:isDefinedBy": {
        "@id": "http://data.vlaanderen.be/ns/persoon"
