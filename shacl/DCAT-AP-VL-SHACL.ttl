@prefix sh:      <http://www.w3.org/ns/shacl#> .
@prefix qb:      <http://purl.org/linked-data/cube#> .
<https://data.vlaanderen.be/shacl/DCAT-AP-VL#DatasetCatalogusShape>
	a sh:NodeShape ;
	sh:targetClass <http://www.w3.org/ns/dcat#Catalog> ;
	sh:property [
		sh:name "heeftDataset" ;
		sh:description "TODO" ;
		sh:path <http://www.w3.org/ns/dcat#dataset> ;
		sh:class <http://www.w3.org/ns/dcat#Dataset> ;
	] ;
	sh:property [
		sh:name "heeftLicentie" ;
		sh:description "De licentie van de datasetcatalogus." ;
		sh:path <http://purl.org/dc/terms/license> ;
		sh:class <http://purl.org/dc/terms/LicenseDocument> ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
	] ;
	sh:property [
		sh:name "heeftUitgever" ;
		sh:description "de uitgever van de datasetcatalogus" ;
		sh:path <http://purl.org/dc/terms/publisher> ;
		sh:class <http://xmlns.com/foaf/0.1/Agent> ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
	] ;
	sh:property [
		sh:name "beschrijving" ;
		sh:description "Een bondige omschrijving van de datasetcatalogus. " ;
		sh:path <http://purl.org/dc/terms/description> ;
		sh:datatype <http://www.w3.org/1999/02/22-rdf-syntax-ns#langString> ;
		sh:minCount 1 ;
		sh:maxCount n ;
	] ;
	sh:property [
		sh:name "titel" ;
		sh:description "De naam van de datasetcatalogus." ;
		sh:path <http://purl.org/dc/terms/title> ;
		sh:datatype <http://www.w3.org/1999/02/22-rdf-syntax-ns#langString> ;
		sh:minCount 1 ;
		sh:maxCount n ;
	] ;
	sh:property [
		sh:name "laatste wijziging" ;
		sh:description "Het meest recente moment waarop de metadata in de datasetcatalogus het laatst is gewijzigd." ;
		sh:path <http://purl.org/dc/terms/modified> ;
		sh:datatype <http://www.w3.org/2001/XMLSchema#dateTime> ;
		sh:maxCount 1 ;
	] ;
	sh:closed false .

