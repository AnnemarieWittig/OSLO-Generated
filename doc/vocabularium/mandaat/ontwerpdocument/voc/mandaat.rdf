<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://www.w3.org/2004/02/skos/core#Concept' />

</owl:ObjectProperty>

<owl:ObjectProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#heeftKandidaat'>
<rdfs:label xml:lang='nl'>heeft kandidaat</rdfs:label>

<rdfs:comment xml:lang='nl'>Een persoon die deel uitmaakt van de lijst.</rdfs:comment>

<rdfs:domain rdf:resource='http://data.vlaanderen.be/ns/mandaat#Kandidatenlijst' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://www.w3.org/ns/person#Person' />

</owl:ObjectProperty>

<owl:ObjectProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#isAangesteldAls'>
<rdfs:label xml:lang='nl'>is aangesteld als</rdfs:label>

<rdfs:comment xml:lang='nl'>Een mandatarisrol die eigen is aan de persoon.</rdfs:comment>

<rdfs:domain rdf:resource='http://www.w3.org/ns/person#Person' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://data.vlaanderen.be/ns/mandaat#Mandataris' />

</owl:ObjectProperty>

<owl:ObjectProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#isAangesteldDoor'>
<rdfs:label xml:lang='nl'>is aangesteld door</rdfs:label>

<rdfs:comment xml:lang='nl'>De rechtsgrond die de aanstelling van de mandataris vastlegt.</rdfs:comment>

<rdfs:domain rdf:resource='http://data.vlaanderen.be/ns/mandaat#Mandataris' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://data.vlaanderen.be/ns/mandaat#RechtsgrondAanstelling' />

</owl:ObjectProperty>

<owl:ObjectProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#isBestuurlijkeAliasVan'>
<rdfs:label xml:lang='nl'>is bestuurlijke alias van</rdfs:label>

<rdfs:comment xml:lang='nl'>De persoon die de mandatarisrol vervult.</rdfs:comment>

<rdfs:domain rdf:resource='http://data.vlaanderen.be/ns/mandaat#Mandataris' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://www.w3.org/ns/person#Person' />

</owl:ObjectProperty>

<owl:ObjectProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#isOntslagenDoor'>
<rdfs:label xml:lang='nl'>is ontslagen door</rdfs:label>

<rdfs:comment xml:lang='nl'>De rechtsgrond die het ontslag van de mandataris vastlegt.</rdfs:comment>

<rdfs:domain rdf:resource='http://data.vlaanderen.be/ns/mandaat#Mandataris' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://data.vlaanderen.be/ns/mandaat#RechtsgrondBeeindiging' />

</owl:ObjectProperty>

<owl:ObjectProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#isResultaatVan'>
<rdfs:label xml:lang='nl'>is resultaat van</rdfs:label>

<rdfs:comment xml:lang='nl'>De persoon horende bij het resultaat.</rdfs:comment>

<rdfs:domain rdf:resource='http://data.vlaanderen.be/ns/mandaat#Verkiezingsresultaat' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://www.w3.org/ns/person#Person' />

</owl:ObjectProperty>

<owl:ObjectProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#isResultaatVoor'>
<rdfs:label xml:lang='nl'>is resultaat voor</rdfs:label>

<rdfs:comment xml:lang='nl'>De lijst waarop het resultaat betrekking heeft.</rdfs:comment>

<rdfs:domain rdf:resource='http://data.vlaanderen.be/ns/mandaat#Verkiezingsresultaat' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://data.vlaanderen.be/ns/mandaat#Kandidatenlijst' />

</owl:ObjectProperty>

<owl:ObjectProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#isTijdelijkVervangenDoor'>
<rdfs:label xml:lang='nl'>is tijdelijk vervangen door</rdfs:label>

<ns2:usageNote xml:lang='nl'>De periode van vervanging komt overeen met de start-/einddatum van de opname van het mandaat van de vervanger.</ns2:usageNote>

<rdfs:comment xml:lang='nl'>Duidt een mandataris aan die tijdelijk de uitvoering van het mandaat heeft overgenomen.</rdfs:comment>

<rdfs:domain rdf:resource='http://data.vlaanderen.be/ns/mandaat#Mandataris' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://data.vlaanderen.be/ns/mandaat#Mandataris' />

</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#lijstnummer'>
<rdfs:label xml:lang='nl'>lijstnummer</rdfs:label>

<rdfs:comment xml:lang='nl'>Het volgnummer van de lijst.</rdfs:comment>

<rdfs:domain rdf:resource='http://data.vlaanderen.be/ns/mandaat#Kandidatenlijst' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://www.w3.org/2001/XMLSchema#integer' />

</owl:DatatypeProperty>

<owl:ObjectProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#lijsttype'>
<rdfs:label xml:lang='nl'>lijsttype</rdfs:label>

<ns2:usageNote xml:lang='nl'>Bijvoorbeeld: gemeenteraadsverkiezing, effectieven Vlaams Parlement.</ns2:usageNote>

<rdfs:comment xml:lang='nl'>Het type van de lijst.</rdfs:comment>

<rdfs:domain rdf:resource='http://data.vlaanderen.be/ns/mandaat#Kandidatenlijst' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://www.w3.org/2004/02/skos/core#Concept' />

</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#plaatsRangorde'>
<rdfs:label xml:lang='nl'>plaats rangorde</rdfs:label>

<rdfs:comment xml:lang='nl'>De positie in de volgorde op de lijst van effectief verkozenen of op de lijst van opvolgers, op basis van het aantal naamstemmen van de kandidaat plus het aantal overgedragen lijststemmen.</rdfs:comment>

<rdfs:domain rdf:resource='http://data.vlaanderen.be/ns/mandaat#Verkiezingsresultaat' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://www.w3.org/2001/XMLSchema#integer' />

</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#rangorde'>
<rdfs:label xml:lang='nl'>rangorde</rdfs:label>

<rdfs:comment xml:lang='nl'>Duidt de plaats van de mandataris aan in de formeel vastgelegde volgorde van de mandatarissen binnen het bestuursorgaan waar de mandataris in zetelt.</rdfs:comment>

<rdfs:domain rdf:resource='http://data.vlaanderen.be/ns/mandaat#Mandataris' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://www.w3.org/1999/02/22-rdf-syntax-ns#langString' />

</owl:DatatypeProperty>

<owl:ObjectProperty rdf:about='http://data.vlaanderen.be/ns/mandaat#redenVanBeeindiging'>
<rdfs:label xml:lang='nl'>reden van beëindiging</rdfs:label>

<rdfs:comment xml:lang='nl'>De reden waarom deze rechtsgrond werd beëindigd.</rdfs:comment>

