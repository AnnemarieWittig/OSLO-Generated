  ];
  sh:targetClass <http://data.europa.eu/m8g/PeriodOfTime> .

<https://data.vlaanderen.be/shacl/TIOD-IM#VergunningShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:datatype xsd:dateTime;
    sh:description "Datum waarop de vergunning aangevraagd wordt.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "aanvraagDatum";
    sh:path <http://data.vlaanderen.be/ns/besluit#aanvraagDatum>
  ], [
    sh:class dc:Agent;
    sh:description "Wie de vergunning aanvraagt.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "aanvrager";
    sh:path <http://data.vlaanderen.be/ns/besluit#aanvrager>
  ], [
    sh:class <http://data.europa.eu/m8g/PeriodOfTime>;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "tijdsinterval";
    sh:path dc:temporal
  ], [
    sh:datatype xsd:boolean;
    sh:description "Duidt aan of  er belastingen op de vergunning betaald zal moeten worden.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "belastingsplichtig";
    sh:path <http://data.vlaanderen.be/ns/besluit#belastingsplichtig>
  ], [
    sh:class dc:Agent;
    sh:description "Persoon of organisatie die de uitvoering doet waarvoor de vergunning toelating gegeven heeft.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "uitvoerder";
    sh:path <http://data.vlaanderen.be/ns/besluit#uitvoerder>
  ], [
    sh:datatype xsd:string;
    sh:description "Waarvoor de vergunning nodig is.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "reden";
    sh:path <http://data.vlaanderen.be/ns/besluit#reden>
  ], [
    sh:class dc:Agent;
    sh:description "Persoon of organisatie die opdracht gegeven heeft om een vergunning aan te vragen.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "opdrachtgever";
    sh:path <http://data.vlaanderen.be/ns/besluit#opdrachtgever>
  ], [
    sh:datatype xsd:dateTime;
    sh:description "Datum waarop de vergunning is goedgekeurd.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "vergunningsdatum";
    sh:path <http://data.vlaanderen.be/ns/besluit#vergunningsdatum>
  ];
  sh:targetClass <http://data.vlaanderen.be/ns/besluit#Vergunning> .

<https://data.vlaanderen.be/shacl/TIOD-IM#ZoneShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class skos:Concept;
    sh:description "Soort zone.";
