<https://data.vlaanderen.be/shacl/TIOD-IM#InnameShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class <http://data.europa.eu/m8g/PeriodOfTime>;
    sh:description "Wanneer de inname plaatsvindt.";
    sh:name "periode";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#periode>
  ], [
    sh:datatype rdf:langString;
    sh:description "Beschrijving over de inname.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "beschrijving";
    sh:path dc:description
  ], [
    sh:class <http://www.w3.org/ns/locn#Address>;
    sh:description "Hiermee wordt bedoeld dat de inname in de buurt van dit adres doorgaat, bijvoorbeeld voor een put aan de straatkant.";
    sh:name "adresVoorstelling";
    sh:path <http://www.w3.org/ns/locn#address>
  ], [
    sh:datatype xsd:boolean;
    sh:description "Duidt aan of de inname in een gebied met openbaar karakter doorgaat.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "openbaarDomein";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#Inname.openbaarDomein>
  ], [
    sh:class org:Organization;
    sh:description "Organisatie die gecontacteerd kan worden over deze inname.";
    sh:name "contactOrganisatie";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#contactOrganisatie>
  ], [
    sh:class <https://data.vlaanderen.be/ns/mobiliteit#Zone>;
    sh:description "Een bepaalde zone die deel uitmaakt van de inname.";
    sh:minCount 1;
    sh:name "zone";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#zone>
  ], [
    sh:class dc:Agent;
    sh:description "Bijdrager aan de inname.";
    sh:name "bijdrager";
    sh:path dc11:contributor
  ], [
    sh:class dc:Agent;
    sh:description "Beheerder van de inname.";
    sh:maxCount 1;
    sh:name "beheerder";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#beheerder>
  ], [
    sh:class schema:URL;
    sh:description "Een webadres met meer informatie over de inname.";
    sh:name "url";
    sh:path schema:url
  ], [
    sh:class <https://data.vlaanderen.be/ns/mobiliteit#Signalisatievergunning>;
    sh:description "Signalisatievergunning die vereist is om deze inname uit te voeren.";
    sh:maxCount 1;
    sh:name "vereistSignalisatievergunning";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#vereistSignalisatievergunning>
  ], [
    sh:datatype rdf:langString;
    sh:description "Tekstuele weergave van de locatie van de inname.";
    sh:maxCount 1;
    sh:name "plaatsbepaling";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#plaatsbepaling>
  ], [
    sh:class skos:Concept;
    sh:description "Status van de inname";
    sh:maxCount 1;
    sh:name "status";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#Inname.status>
  ], [
    sh:class <http://www.w3.org/ns/adms#Identifier>;
    sh:description "Identificator van de inname.";
    sh:name "identificator";
    sh:path <http://www.w3.org/ns/adms#identifier>
  ], [
    sh:class schema:Schedule;
    sh:description "Tijdschema wanneer de inname doorgaat.";
    sh:name "tijdschema";
    sh:path schema:eventSchedule
  ], [
    sh:class skos:Concept;
    sh:description "Type van inname";
    sh:minCount 1;
    sh:name "type";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#Inname.type>
  ];
  sh:targetClass <https://data.vlaanderen.be/ns/mobiliteit#Inname> .

<https://data.vlaanderen.be/shacl/TIOD-IM#InnametypeShape> a sh:NodeShape;
